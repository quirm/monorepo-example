{
  "name": "repo-example",
  "private": true,
  "scripts": {
    "build": "turbo run build --no-daemon",
    "clean": "git clean -Xdfi -e '!*.env*' -e '!.dev.vars'",
    "dev": "turbo run dev --no-daemon",
    "lint": "turbo run lint --no-daemon",
    "typecheck": "turbo run typecheck --no-daemon",
    "format": "prettier --check \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "turbo": "^2.3.3",
    "typescript": "5.6.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
